<rule name="JenkinsProxy" stopProcessing="true">
  <match url="^jenkins/(.*)" />
  <action type="Rewrite" url="http://localhost:8080/jenkins/{R:1}" appendQueryString="true" />
</rule>

<rule name="NextjsProxy" stopProcessing="true">
  <match url="^(.*)$" />
  <conditions logicalGrouping="MatchAll">
    <add input="{REQUEST_URI}" pattern="^/jenkins/" negate="true" />
  </conditions>
  <action type="Rewrite" url="http://localhost:3000/{R:1}" appendQueryString="true" />
</rule>
